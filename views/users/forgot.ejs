<% layout('layouts/boilerplate') -%>





<div class="container mt-5">
    <h1 class="my-5">Forgot Password</h1>
    <form action="/forgot-password?_method=PUT" method="POST">
	<div class="form-group w-50">
		<label for="email">Email</label>
		<input type="email" id="email" name="email" class="form-control" autofocus>
	</div>
	
	<div>
		<button class="btn btn-sm btn-info">Reset Password</button>
	</div>
  </form>
</div>